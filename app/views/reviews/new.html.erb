<!-------------LEAVE A REVIEW----------------->
<h1>Review</h1>
<%= form_with model: @review do |form| %>
  <div>
    <%= form.label :reservation_id %>
    <%= form.number_field :reservation_id %>
  </div>
  <div>
    <%= form.label :rating %>
    <%= form.number_field :rating %>
  </div>
  <div>
    <%= form.label :comment %>
    <%= form.text_area :comment %>
  </div>
    <%= form.submit %>
  <% end %>
  
   